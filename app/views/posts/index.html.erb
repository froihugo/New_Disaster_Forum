<table>
  <thead>
  <td>title</td>
  <td>content</td>
  <td>location</td>
  </thead>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td><%= post.location %></td>
    </tr>
  <% end %>
</table>

