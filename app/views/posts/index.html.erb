<table class="table table-striped table-light">
  <thead>
  <tr>
    <th scope="col">TITLE</th>
    <th scope="col">DESCRIPTION</th>
    <th scope="col">LOCATION</th>
  </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
  <tr>
    <th scope="row"><%= post.title %></th>
    <td><%= post.content %></td>
    <td><%= post.location %></td>
  </tr>
  <% end %>
  </tbody>
</table>